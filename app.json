{
  "expo": {
    "name": "espresso-shot-timer",
    "slug": "espresso-shot-timer",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#050505"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.janhartje.espressoshottimer",
      "versionCode": 9,
      "blockedPermissions": [
        "android.permission.ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.janhartje.espressoshottimer",
      "buildNumber": "8",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png",
          "dark": {
            "backgroundColor": "#050505",
            "image": "./assets/splash-icon.png"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This allows us to measure the effectiveness of our advertising campaigns and improve the app integration with third-party services."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "51f58e15-5883-4bb1-85ec-75cf3c2474bb"
      }
    }
  }
}
